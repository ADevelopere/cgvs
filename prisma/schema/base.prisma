generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearchPostgres"]
}

generator typegraphql {
  provider            = "typegraphql-prisma"
  output              = "../../graphql/generated/type-graphql"
  formatGeneratedCode = "prettier"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
